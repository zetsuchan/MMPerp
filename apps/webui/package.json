    "oxc-resolver": "^11.17.0",
    "oxc-transform": "^0.112.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  }
}
